<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net"  xmlns:aop="http://www.springframework.net/aop">
  <object id="homeController" type="WMS.AuthorizationServer.Controllers.HomeController, WMS.AuthorizationServer" singleton="false" >
  </object>

  <object id="accountController" type="WMS.AuthorizationServer.Controllers.AccountController, WMS.AuthorizationServer" singleton="false" >
    <property name="userProfileDao" ref="userProfileDao"></property>
  </object>
		<object id="authorizeController" type="WMS.AuthorizationServer.Controllers.AuthorizeController, WMS.AuthorizationServer" singleton="false" >
				<property name="clientService" ref="clientService"></property>
				<property name="securityService" ref="securityService"></property>
		</object>
		<object id="tokenController" type="WMS.AuthorizationServer.Controllers.TokenController, WMS.AuthorizationServer" singleton="false" >
				<property name="clientService" ref="clientService"></property>
				<property name="securityService" ref="securityService"></property>
		</object>
</objects>